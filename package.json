{
    "browserslist": {
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ],
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ]
    },
    "dependencies": {
        "@supabase/supabase-js": "1.24.0",
        "evergreen-ui": "6.5.1",
        "immutable": "4.0.0",
        "jotai": "1.3.9",
        "lodash": "4.17.21",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-query": "3.22.0",
        "react-router": "5.2.1",
        "react-router-config": "5.1.1",
        "react-router-dom": "5.3.0",
        "react-scripts": "4.0.3",
        "reactronica": "0.6.2",
        "rooks": "5.7.2",
        "slugify": "1.6.0",
        "tone": "14.7.77",
        "typescript": "4.4.4",
        "upath": "1.2.0",
        "uuid": "8.3.2",
        "web-vitals": "1.1.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/react": "11.2.7",
        "@testing-library/user-event": "12.8.3",
        "@types/jest": "26.0.24",
        "@types/lodash": "4.14.172",
        "@types/node": "12.20.19",
        "@types/react": "17.0.18",
        "@types/react-dom": "17.0.9",
        "@types/react-router-config": "5.0.3",
        "@types/react-router-dom": "5.1.8",
        "@types/uuid": "8.3.1",
        "node-pg-migrate": "5.10.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "name": "beets",
    "private": true,
    "scripts": {
        "build": "react-scripts build",
        "build:migrations": "echo Building src/scripts/migrations && echo Using TypeScript && tsc -v && tsc --project src/scripts/migrations",
        "eject": "react-scripts eject",
        "premigrations:create": "npm run build:migrations",
        "premigrations:run": "npm run build:migrations",
        "migrations:create": "source .env && echo Using node-pg-migrate from && which node-pg-migrate && node-pg-migrate --envPath .env  --migrations-dir src/scripts/migrations --template-file-name src/scripts/migrations/utils/migration-template.ts --quote-escape false create",
        "migrations:run": "source .env && echo Using node-pg-migrate from && which node-pg-migrate && node-pg-migrate --envPath .env --migrations-dir src/scripts/migrations/dist --quote-escape false",
        "start": "react-scripts start",
        "test": "react-scripts test",
        "update-supabase-types": "source .env && npx openapi-typescript $REACT_APP_SUPABASE_URL/rest/v1/?apikey=$REACT_APP_SUPABASE_ANON_KEY --output src/types/supabase.ts"
    },
    "version": "0.1.0"
}
